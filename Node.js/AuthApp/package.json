    "node-fetch": "^1.7.3",
    "pbkdf2-password": "^1.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "nodemon": "^1.12.5"
  },
  "repository": {
    "type": "git",
